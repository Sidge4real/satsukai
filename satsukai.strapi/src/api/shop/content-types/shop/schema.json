{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "singularName": "shop",
    "pluralName": "shops",
    "displayName": "Shop",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "info": {
      "type": "text"
    },
    "amount_of_items": {
      "type": "integer",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "origin": {
      "type": "enumeration",
      "enum": [
        "Japan",
        "Korea",
        "China",
        "Taiwan",
        "Indonesia",
        "India",
        "Nepal",
        "Vietnam",
        "Mongolia",
        "Russia",
        "Thailand",
        "Philippines",
        "Singapore",
        "Cambodja",
        "Laos",
        "Myanmar",
        "Bangladesh",
        "Bhutan",
        "Papoea-Nieuw-Guinea"
      ]
    },
    "indoor": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "min_temp": {
      "type": "decimal",
      "required": true
    },
    "max_temp": {
      "type": "decimal"
    },
    "age": {
      "type": "integer"
    },
    "max_age": {
      "type": "integer"
    },
    "showroom": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
